# Airtable Public API key (for listing users)
AIRTABLE_API_KEY=patXXXXXXXXXXXXXXX.XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX

# Enterprise account ID
AIRTABLE_ENTERPRISE_ID=entXXXXXXXXXXXXXX

# Session cookie from browser (the entire Cookie header value)
SENTRY_DSN=
AIRTABLE_SESSION_COOKIE="__Host-airtable-session=XXX; __Host-airtable-session.sig=XXX; ..."

# CSRF token (from the session cookie, decode base64 to get csrfSecret)
AIRTABLE_CSRF_TOKEN=XXXXXXXXXXXX

# API token for authenticating requests to /revoke
API_TOKEN=your_secret_api_token

# Server port
PORT=4567
